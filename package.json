{
  "name": "alphonso-server",
  "version": "0.0.1",
  "description": "An open source web based administrative server interface for MongoDB",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphonso-db/alphonso-server.git"
  },
  "scripts": {
    "start": "cross-env BABEL_ENV=development babel-node ./src/server.js",
    "start:prod": "cross-env BABEL_ENV=production babel-node ./src/server.js",
    "start:cluster": "cross-env BABEL_ENV=production NODE_DEBUG=cluster babel-node ./src/cluster.js",
    "start:nodemon": "nodemon ./src/server.js --exec babel-node --presets es2015",
    "start:debug": "cross-env BABEL_ENV=development babel-node --debug-brk ./src/server.js",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "nspcheck": "nsp check"
  },
  "keywords": [
    "database",
    "mongodb",
    "nosql",
    "framework",
    "mongodb-server"
  ],
  "author": "Ashwin Hegde",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alphonso-db/alphonso-server/issues"
  },
  "homepage": "https://github.com/alphonso-db/alphonso-server#readme",
  "dependencies": {
    "co": "^4.6.0",
    "elasticsearch": "^11.0.1",
    "es6-promise": "^3.2.1",
    "events": "^1.1.0",
    "graphiql": "^0.8.1",
    "jsonwebtoken": "^7.2.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.3.0",
    "koa-compress": "^1.0.9",
    "koa-cors": "0.0.16",
    "koa-error": "^2.1.0",
    "koa-graphiql": "^1.1.0",
    "koa-helmet": "^2.0.0",
    "koa-jwt": "^1.2.0",
    "koa-logger": "^1.3.0",
    "koa-mongo": "^1.1.0",
    "koa-route": "^2.4.2",
    "koa-send": "^3.2.0",
    "koa-session": "^3.4.0",
    "koa-static": "^2.0.0",
    "koa-static-cache": "^3.1.7",
    "koa-validate": "^1.0.7",
    "moment": "^2.17.1",
    "mongodb": "^2.2.19",
    "nodemailer": "^2.7.0",
    "object-assign": "^4.1.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "superagent": "^2.3.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-jest": "^12.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^5.1.1",
    "mocha": "^3.2.0",
    "node-inspector": "^0.12.8",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.2",
    "rimraf": "^2.5.4",
    "supertest": "^2.0.1"
  }
}
